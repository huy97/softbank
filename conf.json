{
    "source": {
        "include": ["./dist/lib/", "./package.json"]
    },
    "plugins": ["plugins/markdown"],
    "templates": {
        "default": {
            "includeDate": false
        }
    },
    "opts": {
        "destination": "./docs",
        "template": "node_modules/clean-jsdoc-theme",
        "theme_opts": {
            "title": "softbank-js",
            "resizeable": {
                "navbar": {
                    "min": "320",
                    "max": "600"
                }
            },
            "menu": [
                {
                    "title": "Getting Started",
                    "link": "./index.html"
                },
                {
                    "title": "GitHub repository",
                    "link": "https://github.com/huy97/softbank.git"
                }
            ]
        }
    }
}
